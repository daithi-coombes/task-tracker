{
  "name": "task-tracker",
  "version": "0.0.1",
  "description": "Series of bash scripts and nodejs apps to research monitoring user actions to predict tasks completed by grouping recorded events",
  "main": "index.js",
  "scripts": {
    "test":         "echo \"Error: no test specified\" && exit 1",
    "lint":         "./node_modules/.bin/jshint . --reporter=./node_modules/jshint-full-path/index.js",
    "checkStyle":   "./node_modules/.bin/jscs .",
    "pretest":      "npm run-script lint && npm run-script checkStyle",
    "test":         "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive -R spec -r should",
    "posttest":     "./node_modules/.bin/istanbul check-coverage && rm -rf coverage"
  },
  "dependencies": {
    "fs": "latest"
  },
  "devDependencies": {
    "mocha":      "latest",
    "should":     "latest",
    "sinon":      "latest",
    "nock":       "latest",
    "supertest":  "latest",
    "rewire":     "latest",
    "jsdoc":      "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/daithi-coombes/task-tracker.git"
  },
  "keywords": [
    "project",
    "management",
    "task",
    "tasking",
    "tracking"
  ],
  "author": "daithi coombes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daithi-coombes/task-tracker/issues"
  },
  "homepage": "https://github.com/daithi-coombes/task-tracker#readme"
}
